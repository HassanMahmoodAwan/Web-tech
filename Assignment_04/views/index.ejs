<!-- Products -->


<h1 class="my-5">Prodcuts</h1>
<div class="container d-flex justify-content-center mb-5">
<% for(let prod in product){ %>
<div class="card mx-2 rounded" style="width: 18rem;">
  <img class="card-img-top" src="/images/p<%= Number(prod)+1 %>.png" alt="Card image cap">
  <div class="card-body">
    <h3 class="card-title"><%= product[prod].Name %></h3>

    
    <div class="d-flex justify-content-between mx-2">
        <p><%= product[prod].Company %></p>
        <p><%= product[prod].Category %></p>
    </div>
    <p class="card-text">Some quick example text to build on the card.</p>

    <h5 class="mt-2 mb-3 mx-3 ">$ <%= product[prod].Price %></h5>

    <a href="#" class="btn btn-primary">Add to Cart</a>
  </div>
</div>
<% } %>
</div>





